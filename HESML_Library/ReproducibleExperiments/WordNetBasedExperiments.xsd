		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CorpusBasedICModelType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Resnik"/>
			<xs:enumeration value="CondProbCorpus"/>
			<xs:enumeration value="CondProbRefCorpus"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="CorpusICmodelsExperiment">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="WordNetBasedExperiment">
					<xs:sequence>
						<xs:element name="ConceptFrequencyFilesDir"/>
						<xs:element name="ICmodels">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ICmodel" type="CorpusBasedICModelType" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ConceptFrequencyFiles">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ConceptFrequencyFile" type="xs:string" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="SimilarityMeasures">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="SimilarityMeasure" type="SimilarityMeasureType" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="CorrelationOutputMetrics">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Pearson"/>
			<xs:enumeration value="Spearman"/>
			<xs:enumeration value="PearsonAndSpearman"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="MixedICmodelsExperiment">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="WordNetBasedExperiment">
					<xs:sequence>
						<xs:element name="ConceptFrequencyFilesDir"/>
						<xs:element name="SimilarityMeasures">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="SimilarityMeasure" type="SimilarityMeasureType" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ICmodels">
							<xs:complexType>
								<xs:choice maxOccurs="unbounded">
									<xs:element name="IntrinsicICModel" type="IntrinsicICModelType" minOccurs="0" maxOccurs="unbounded"/>
									<xs:element name="CorpusBasedICModel" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Method" type="CorpusBasedICModelType"/>
												<xs:element name="ConceptFrequencyFilename" type="xs:string"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="MultipleDatasetsExperiment">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OutputFileName" type="xs:string"/>
				<xs:element name="DatasetDirectory" type="xs:string"/>
				<xs:element name="ConceptFrequencyFilesDir"/>
				<xs:element name="CorrelationOutputMetrics" type="CorrelationOutputMetrics"/>
				<xs:element name="WordNetDatabaseFileName" type="xs:string"/>
				<xs:element name="SimilarityMeasures">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SpecificSimilarityMeasure" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="MeasureType" type="SimilarityMeasureType"/>
										<xs:choice minOccurs="0">
											<xs:element name="IntrinsicICModel" type="IntrinsicICModelType" minOccurs="0"/>
											<xs:element name="CorpusBasedICModel" minOccurs="0">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="Method" type="CorpusBasedICModelType"/>
														<xs:element name="ConceptFrequencyFilename" type="xs:string"/>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="WordNetVersions">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="WordNetDatabaseDirectory" type="xs:string" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="WNDatasets">
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="DatasetFileName" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SingleDatasetSimilarityValuesExperiment">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OutputFileName" type="xs:string"/>
				<xs:element name="DatasetDirectory" type="xs:string"/>
				<xs:element name="DatasetFileName" type="xs:string"/>
				<xs:element name="WordNetMeasures" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="WordNetDatabaseFileName" type="xs:string"/>
							<xs:element name="WordNetDatabaseDirectory" type="xs:string"/>
							<xs:element name="SimilarityMeasures">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="SpecificSimilarityMeasure" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="MeasureType" type="SimilarityMeasureType"/>
													<xs:element name="IntrinsicICModel" type="IntrinsicICModelType" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="RawWordVectorFiles" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="EmbVectorFiles">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="VectorFile" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="UKBVectorFiles">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="VectorFile" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="NasariVectorFiles">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="NasariVectorFile" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SensesFile" type="xs:string"/>
													<xs:element name="VectorFile" type="xs:string"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
